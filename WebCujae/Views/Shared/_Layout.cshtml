                    <img src="~/Content/Home/img/test.jpg" style="height:120px" a class="navbar-brand img-circle " />
                    <!--  <a href="#body">Blue</a>-->
                    <!-- /logo -->
                </div>

                <!-- main nav -->
                <nav class="collapse navbar-collapse navbar-right" role="navigation" style="margin-top:3%;">
                          <div class="footer-social">
                        <ul>
                            <li class="wow animated zoomIn iconwhite">@Html.ActionLink(" ", "Login", "Account", null, new { @class = "fa fa-cog fa-2x" })</li>
                            <li class="wow animated zoomIn iconwhite" data-wow-delay="0.3s"><a href="#"><i class="fa fa-twitter fa-2x"></i></a></li>
