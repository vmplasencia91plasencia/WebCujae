                        </ul>
                    </div>
                    <ul class="nav navbar-nav">
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Estudio</a>
                            <ul class="dropdown-menu" role="menu">
                                <li>@Html.ActionLink("Pregrado", "Pregrado", "Home", null, null)</li>
                                <li><a href="#">Postgrado</a></li>
                            </ul>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Investigacion </a>
                            <ul class="dropdown-menu" role="menu">
                                <li><a href="#">Publicaciones</a></li>
                                <li><a href="#">Lineas de Investigaci&oacute;n</a></li>
                                <li><a href="#">Proyectos de Investigaci&oacute;n</a></li>
                                <li class="divider"></li>
                                <li><a href="#">Cooperaci&oacute;n</a></li>
                            </ul>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Vida Universitaria </a>
                            <ul class="dropdown-menu" role="menu">
                                <li><a href="#">Eventos deportivos</a></li>
                                <li><a href="#">Cultura</a></li>
                                <li><a href="#">Recreaci&oacute;n</a></li>
                            </ul>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">Internacional</a>
                            <ul class="dropdown-menu" role="menu">
                                <li><a href="#">Proyectos</a></li>
